  },
  "homepage": "https://github.com/inomag/remixicon-react-bundler/blob/master/README.md",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "tsx scripts/updateTypesAndString.ts",
    "build": "tsc && webpack"
